<web-app id="WebApp_9" version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

     <servlet>
	   <display-name>Solr Insert Servlet</display-name>
	   <servlet-name>testServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.TestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>Solr Sprawler Servlet</display-name>
	   <servlet-name>sprawlerServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SprawlerServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>Solr Sprawler Ping Servlet</display-name>
	   <servlet-name>sprawlerPingServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SprawlerPingServlet</servlet-class>
	 </servlet>
	 
	 
	 
     <servlet-mapping>
	   <servlet-name>testServlet</servlet-name>
	   <url-pattern>/test.do</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	   <servlet-name>sprawlerServlet</servlet-name>
	   <url-pattern>/sprawler.do</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	   <servlet-name>sprawlerPingServlet</servlet-name>
	   <url-pattern>/sprawlerPing.do</url-pattern>
	 </servlet-mapping>
	 
	 
	 

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	 
</web-app>
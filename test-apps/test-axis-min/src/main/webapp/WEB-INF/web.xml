<web-app id="WebApp_9" version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

     <servlet>
	   <display-name>Weather Servlet</display-name>
	   <servlet-name>weatherServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.WeatherServlet</servlet-class>
	 </servlet>
	 
     <servlet>
	   <display-name>Sample REST Servlet</display-name>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleRestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>Sample REST Async Servlet</display-name>
	   <servlet-name>sampleRestAsyncServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleRestAsyncServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>Sample SOAP Servlet</display-name>
	   <servlet-name>sampleSoapServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleSoapServlet</servlet-class>
	 </servlet>
	 
 	 <servlet>
	   <display-name>Sample SOAP Async Servlet</display-name>
	   <servlet-name>sampleSoapAsyncServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleSoapAsyncServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>API REST Servlet</display-name>
	   <servlet-name>apiRestServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.ApiRestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <servlet-name>simple.jsp</servlet-name>
	   <jsp-file>/simple.jsp</jsp-file>
	 </servlet>
	 
	 <servlet-mapping>
	   <servlet-name>weatherServlet</servlet-name>
	   <url-pattern>/weather.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <url-pattern>/sampleRest.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>sampleRestAsyncServlet</servlet-name>
	   <url-pattern>/sampleRestAsync.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>sampleSoapServlet</servlet-name>
	   <url-pattern>/sampleSoap.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>sampleSoapAsyncServlet</servlet-name>
	   <url-pattern>/sampleSoapAsync.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>apiRestServlet</servlet-name>
	   <url-pattern>/apiRest.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>simple.jsp</servlet-name>
	   <url-pattern>/simple.do</url-pattern>
	 </servlet-mapping>
	 
	 

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	 
</web-app>
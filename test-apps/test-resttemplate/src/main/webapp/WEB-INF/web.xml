<web-app id="WebApp_9" version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	 <servlet>
	   <display-name>Sample REST Servlet</display-name>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleRestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>Sample REST async Servlet</display-name>
	   <servlet-name>sampleRestAsyncServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleRestAsyncServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <display-name>API REST Servlet</display-name>
	   <servlet-name>apiRestServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.ApiRestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <servlet-name>simple.jsp</servlet-name>
	   <jsp-file>/simple.jsp</jsp-file>
	 </servlet>
	 
	 <servlet-mapping>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <url-pattern>/sampleRest.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>sampleRestAsyncServlet</servlet-name>
	   <url-pattern>/sampleRestAsync.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>apiRestServlet</servlet-name>
	   <url-pattern>/apiRest.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>simple.jsp</servlet-name>
	   <url-pattern>/simple.do</url-pattern>
	 </servlet-mapping>

	<welcome-file-list>
		<welcome-file>simple.do</welcome-file>
	</welcome-file-list>
	 
</web-app>
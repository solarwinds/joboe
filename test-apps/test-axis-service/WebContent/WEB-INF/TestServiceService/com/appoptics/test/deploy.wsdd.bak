<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestServiceService WSDL service -->

  <service name="TestService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://test.appoptics.com"/>
      <parameter name="wsdlServiceElement" value="TestServiceService"/>
      <parameter name="schemaQualified" value="http://test.appoptics.com"/>
      <parameter name="wsdlServicePort" value="TestService"/>
      <parameter name="className" value="com.appoptics.test.TestServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TestService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="testException" qname="operNS:testException" xmlns:operNS="http://test.appoptics.com" returnQName="retNS:testExceptionReturn" xmlns:retNS="http://test.appoptics.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:input" xmlns:pns="http://test.appoptics.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="helloWorld" qname="operNS:helloWorld" xmlns:operNS="http://test.appoptics.com" returnQName="retNS:helloWorldReturn" xmlns:retNS="http://test.appoptics.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="processSomething" qname="operNS:processSomething" xmlns:operNS="http://test.appoptics.com" returnQName="retNS:processSomethingReturn" xmlns:retNS="http://test.appoptics.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:input" xmlns:pns="http://test.appoptics.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="testException processSomething helloWorld"/>

  </service>
</deployment>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../main/config/ehcache.xsd">
	<diskStore path="auto/default/path" />
	<defaultCache eternal="false"
		timeToIdleSeconds="5" timeToLiveSeconds="10" maxElementsInMemory="100000">
	</defaultCache>
	<!-- Page and Page Fragment Caches -->
	<cache name="CachePage1CachingFilter" 
		eternal="false" timeToIdleSeconds="10000" timeToLiveSeconds="10000" maxElementsInMemory="100000">
	</cache>
	<cache name="CachedPage2Cache" eternal="false"
		timeToLiveSeconds="3600" maxElementsInMemory="100000">
	</cache>
	<cache name="SimplePageFragmentCachingFilter"
		eternal="false" timeToIdleSeconds="10000"
		timeToLiveSeconds="10000" maxElementsInMemory="100000">
	</cache>
	<cache name="SimpleCachingHeadersTimeoutPageCachingFilter"
		eternal="false" timeToIdleSeconds="10000"
		timeToLiveSeconds="10000" maxElementsInMemory="100000">
	</cache>
</ehcache>
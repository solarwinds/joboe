<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
	id="WebApp_ID" version="2.5">

     <servlet>
	   <display-name>Sample REST Servlet</display-name>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <servlet-class>com.tracelytics.test.SampleRestServlet</servlet-class>
	 </servlet>
	 
	 <servlet>
	   <servlet-name>simple.jsp</servlet-name>
	   <jsp-file>/simple.jsp</jsp-file>
	 </servlet>
	 
	 <servlet-mapping>
	   <servlet-name>sampleRestServlet</servlet-name>
	   <url-pattern>/sampleRest.do</url-pattern>
	 </servlet-mapping>
	 <servlet-mapping>
	   <servlet-name>simple.jsp</servlet-name>
	   <url-pattern>/simple.do</url-pattern>
	 </servlet-mapping> 	 
	 
	 

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	 
</web-app>